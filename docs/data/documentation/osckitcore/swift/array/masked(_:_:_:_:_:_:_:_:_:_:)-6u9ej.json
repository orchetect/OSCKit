text"},{"text":"V2","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V3","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V4","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V5","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V6","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V7"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V8"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V9","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","kind":"typeIdentifier"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"Common usage:","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"Basic examples:"}],"type":"paragraph"},{"syntax":"swift","code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"type":"codeListing"},{"inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"syntax":"swift","code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"type":"codeListing"},{"inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"text":" ","type":"text"},{"text":"be transparently encoded as their closest related OSC core type when encoding in an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCMessage","isActive":true,"type":"reference"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"These types conform to the ","type":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCInterpolatedValue","isActive":true,"type":"reference"},{"text":" protocol (which defines their core","type":"text"},{"text":" ","type":"text"},{"text":"encoding value type) and may be used any where ","type":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValue","isActive":true,"type":"reference"},{"text":" is accepted.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"OSCKit provides conformance for a number of such common Swift Standard Library types:","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"Int8"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt8"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt32"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt64"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float80"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"(Note that ","type":"text"},{"type":"codeVoice","code":"Int32"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int64"},{"text":", ","type":"text"},{"code":"Float32","type":"codeVoice"},{"type":"text","text":" are already core "},{"code":"OSCValue","type":"codeVoice"},{"type":"text","text":" types.)"}],"type":"paragraph"},{"syntax":"swift","code":["let values: OSCValues = [Int8(123), Int16(123)]"],"type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}],"type":"paragraph"},{"syntax":"swift","code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"],"type":"codeListing"},{"inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"code":"Optional","type":"codeVoice"},{"text":" (ie: ","type":"text"},{"code":"Int32?.self","type":"codeVoice"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"which will match that value type or return ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"text":"array.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Core OSC concrete types:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"code":"Int32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Float32.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"String.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Data.self","type":"codeVoice"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}]}]}]},{"inlineContent":[{"text":"Extended (non-standard) OSC concrete types:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Bool.self"}]}]},{"content":[{"inlineContent":[{"code":"Character.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Int64.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Double.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCNullValue.self"}]}]},{"content":[{"inlineContent":[{"code":"OSCStringAltValue.self","type":"codeVoice"}],"type":"paragraph"}]}]},{"inlineContent":[{"text":"Interpolating concrete types for convenience:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" (converts from any OSC integer types)"}]}]}]},{"inlineContent":[{"text":"Opaque types:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue.self"},{"text":" (boxes any OSC integer or float number)","type":"text"}]}]}]