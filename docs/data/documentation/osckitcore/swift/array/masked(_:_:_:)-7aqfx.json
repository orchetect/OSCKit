dules":["Swift"],"name":"OSCKitCore"}],"externalID":"s:Sa10OSCKitCoreAA8OSCValue_pRszlE6maskedyqd__Sg_qd_0_Sgqd_1_SgtADm_AEmAFmtAA0C9MaskErrorOYKAA0C8MaskableRd__AaIRd_0_AaIRd_1_r1_lF","symbolKind":"method"},"hierarchy":{"paths":[["doc:\/\/OSCKitCore\/documentation\/OSCKitCore","doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/Swift","doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/Swift\/Array"]]},"primaryContentSections":[{"declarations":[{"languages":["swift"],"platforms":["macOS"],"tokens":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"masked","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V0","kind":"genericParameter"},{"text":", ","kind":"text"},{"kind":"genericParameter","text":"V1"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V2"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"v0"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":"?.Type, "},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"v1"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":"?.Type, "},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"v2"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V2"},{"kind":"text","text":"?.Type) "},{"kind":"keyword","text":"throws"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"OSCValueMaskError","preciseIdentifier":"s:10OSCKitCore17OSCValueMaskErrorO","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskError"},{"kind":"text","text":") -> ("},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":"?, "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":"?, "},{"kind":"typeIdentifier","text":"V2"},{"kind":"text","text":"?) "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V2"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"}]}],"kind":"declarations"},{"content":[{"text":"Discussion","type":"heading","anchor":"discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"A mask of up to 10 value types in sequence."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Common usage:"}]},{"syntax":"swift","type":"codeListing","code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"]},{"type":"paragraph","inlineContent":[{"text":"Basic examples:","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}]},{"syntax":"swift","type":"codeListing","code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"]},{"type":"paragraph","inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"text":" ","type":"text"},{"text":"be transparently encoded as their closest related OSC core type when encoding in an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCMessage","isActive":true,"type":"reference"},{"text":".","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"These types conform to the "},{"type":"reference","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCInterpolatedValue","isActive":true},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"type":"reference","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValue","isActive":true},{"type":"text","text":" is accepted."}]},{"type":"paragraph","inlineContent":[{"text":"OSCKit provides conformance for a number of such common Swift Standard Library types:","type":"text"},{"text":" ","type":"text"},{"code":"Int8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Int16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt32","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt64","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float80","type":"codeVoice"},{"text":".","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int64"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float32"},{"type":"text","text":" are already core "},{"type":"codeVoice","code":"OSCValue"},{"type":"text","text":" types.)"}]},{"syntax":"swift","type":"codeListing","code":["let values: OSCValues = [Int8(123), Int16(123)]"]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"text":" is a special type to match and box any OSC numeric value.","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"type":"paragraph","inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"type":"codeVoice","code":"Optional"},{"text":" (ie: ","type":"text"},{"type":"codeVoice","code":"Int32?.self"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"which will match that value type or return ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"text":"array.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Core OSC concrete types:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int32.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"Float32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"String.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Data.self","type":"codeVoice"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"code":"Bool.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Character.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Int64.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"Double.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"OSCStringAltValue.self","type":"codeVoice"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" (converts from any OSC integer types)"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"text":"Opaque types:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue.self"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}]},{"style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"type":"reference","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskError","isActive":true}]}],"type":"aside","name":"Throws"}],"kind":"content"}],"schemaVersion":{"minor":3,"major":0,"patch":0},"references":{"doc://OSCKitCore/documentation/OSCKitCore/Swift/Array":{"type":"topic","fragments":[{"text":"extension","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"s:Sa","text":"Array","kind":"identifier"}],"url":"\/documentation\/osckitcore\/swift\/array","title":"Array","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/Swift\/Array","role":"symbol","kind":"symbol","navigatorTitle":[{"text":"Array","kind":"identifier"}],"abstract":[]},"doc://OSCKitCore/documentation/OSCKitCore/OSCValue":{"role":"symbol","abstract":[{"type":"text","text":"Protocol to which all compatible OSC value types conform."}],"type":"topic","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OSCValue","kind":"identifier"}],"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValue","url":"\/documentation\/osckitcore\/oscvalue","navigatorTitle":[{"kind":"identifier","text":"OSCValue"}],"title":"OSCValue","kind":"symbol"},"doc://OSCKitCore/documentation/OSCKitCore/Swift":{"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/Swift","url":"\/documentation\/osckitcore\/swift","abstract":[],"role":"coll