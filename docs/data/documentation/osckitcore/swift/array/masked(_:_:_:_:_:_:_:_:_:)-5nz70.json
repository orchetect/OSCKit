askable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V1","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V2","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V3","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V4","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V5","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V6","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V7","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V8","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskable"}]}]},{"content":[{"anchor":"discussion","text":"Discussion","level":2,"type":"heading"},{"inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Common usage:"}],"type":"paragraph"},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"Basic examples:"}],"type":"paragraph"},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCMessage"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"These types conform to the "},{"isActive":true,"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCInterpolatedValue","type":"reference"},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"isActive":true,"identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValue","type":"reference"},{"type":"text","text":" is accepted."}],"type":"paragraph"},{"inlineContent":[{"text":"OSCKit provides conformance for a number of such common Swift Standard Library types:","type":"text"},{"text":" ","type":"text"},{"code":"Int8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Int16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt32","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt64","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float80","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"(Note that ","type":"text"},{"code":"Int32","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Int64","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float32","type":"codeVoice"},{"text":" are already core ","type":"text"},{"code":"OSCValue","type":"codeVoice"},{"text":" types.)","type":"text"}],"type":"paragraph"},{"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}],"type":"paragraph"},{"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"code":"Optional","type":"codeVoice"},{"text":" (ie: ","type":"text"},{"code":"Int32?.self","type":"codeVoice"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"which will match that value type or return ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"text":"array.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Core OSC concrete types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Float32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"String.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Data.self"},{"type":"text","text":" (a.k.a. OSC blob)"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCTimeTag.self"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"text":"Extended (non-standard) OSC concrete types:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Bool.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Character.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int64.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Double.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCArrayValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCNullValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}]}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" (converts from any OSC integer types)"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Opaque types:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}],"type":"unorderedList"},{"style":"note","name":"Throws","content":[{"inlineContent":[{"text":"","type":"text"},{"type":"reference","identifier":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/OSCValueMaskError","isActive":true}],"type":"paragraph"}],"type":"aside"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/osckitcore\/swift\/array\/masked(_:_:_:_:_:_:_:_:_:)-5nz70"]}],"identifier":{"url":"doc:\/\/OSCKitCore\/documentation\/OSCKitCore\/Swift\/Array\/masked(_:_:_:_:_:_:_:_:_:)-5nz70","interfaceLanguage":"swift"},"kind":"symbol","metadata":{"role":"symbol","roleHeading":"Instance Method","symbolKind":"method","modules":[{"name":"OSCKitCore","relatedModules":["Swift"]}],"title":"masked(_:_:_:_:_:_:_:_:_:)","conformance":{"availabilityPrefix":[{"text":"Available when","type":"text"}],"constraints":[{"type":"codeVoice","code":"Element"},{"text":" is ","type":"text"},{"type":"codeVoice","code":"any OSCValue"},{"text":".","type":"text"}],"conformancePrefix":[{"text":"Conforms when","type":"text"}]},"externalID":"s:Sa10OSCKitCoreAA8OSCValue_pRszlE6maskedyqd___qd_0_qd_1_qd_2_Sgqd_3_Sgqd_4_Sgqd_5_Sgqd_6_Sgqd_7_Sgtqd__m_qd_0_mqd_1_mADmAEmAFmAGmAHmAImtAA0C9MaskErrorOYKAA0C8MaskableRd__AaLRd_0_AaLRd_1_AaLRd_2_AaLRd_3_AaLRd_4_AaLRd_5_AaLRd_6_AaLRd_7_r7_lF","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"masked"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"V0"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V1"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V2"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V3"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V4"},{"kind":"text","text":", "},{"kind":"genericParameter","text":"V5"},{"kind":"text","text":", "},{"kind":"genericParameter"