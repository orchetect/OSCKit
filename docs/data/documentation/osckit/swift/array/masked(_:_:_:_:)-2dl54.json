{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift\/Array\/masked(_:_:_:_:)-2dl54"},"abstract":[{"text":"Returns the OSC value sequence as a strongly typed tuple","type":"text"},{"type":"text","text":" "},{"type":"text","text":"if it matches the given mask of concrete value types."},{"type":"text","text":" "},{"type":"text","text":"If the value types do not match the mask, an error is thrown."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"masked"},{"kind":"text","text":"<"},{"text":"V0","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"V1","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"V2","kind":"genericParameter"},{"kind":"text","text":", "},{"text":"V3","kind":"genericParameter"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"text":" ","kind":"text"},{"kind":"internalParam","text":"v0"},{"kind":"text","text":": "},{"text":"V0","kind":"typeIdentifier"},{"kind":"text","text":".Type, "},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"v1"},{"text":": ","kind":"text"},{"text":"V1","kind":"typeIdentifier"},{"kind":"text","text":".Type, "},{"kind":"externalParam","text":"_"},{"text":" ","kind":"text"},{"text":"v2","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V2","kind":"typeIdentifier"},{"kind":"text","text":".Type, "},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"v3","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V3","kind":"typeIdentifier"},{"kind":"text","text":"?.Type) "},{"kind":"keyword","text":"throws"},{"text":" -> (","kind":"text"},{"text":"V0","kind":"typeIdentifier"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":", "},{"text":"V2","kind":"typeIdentifier"},