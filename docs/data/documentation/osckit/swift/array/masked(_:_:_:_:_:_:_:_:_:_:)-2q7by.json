ier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V3","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V4"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V5"},{"kind":"text","text":" : "},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"kind":"text","text":", "},{"text":"V6","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"},{"kind":"text","text":", "},{"text":"V7","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V8"},{"kind":"text","text":" : "},{"text":"OSCValueMaskable","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"text":"V9","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Common usage:"}]},{"syntax":null,"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"Basic examples:"}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"]},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"syntax":null,"type":"codeListing","code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"]},{"inlineContent":[{"type":"text","text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will"},{"type":"text","text":" "},{"text":"be transparently encoded as their closest related OSC core type when encoding in an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage","type":"reference","isActive":true},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"These types conform to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue"},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"text":"encoding value type) and may be used any where ","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue","isActive":true,"type":"reference"},{"text":" is accepted.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"text":" ","type":"text"},{"code":"Int8","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int16"},{"type":"text","text":", "},{"code":"UInt","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt8"},{"type":"text","text":", "},{"code":"UInt16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt32"},{"type":"text","text":", "},{"code":"UInt64","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float80"},{"text":".","type":"text"}]},{"inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int64"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float32"},{"text":" are already core ","type":"text"},{"code":"OSCValue","type":"codeVoice"},{"type":"text","text":" types.)"}],"type":"paragraph"},{"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"syntax":null,"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}]},{"syntax":null,"type":"codeListing","code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"code":"Int32?.self","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"text":"which will match that value type or return ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"text":"array.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Core OSC concrete types:","type":"text"}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int32.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Float32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"String.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Data.self","type":"codeVoice"},{"type":"text","text":" (a.k.a. OSC blob)"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}]}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Bool.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Character.self"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Double.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCArrayValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"OSCMIDIValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCNullValue.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCStringAltValue.self","type":"codeVoice"}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"Interpolating concrete types for convenience:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" (converts from any OSC integer types)"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"text":"Opaque types:","type":"text"}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}],"type":"unorderedList"},{"type":"aside","name":"Throws","style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"type":"reference","isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\