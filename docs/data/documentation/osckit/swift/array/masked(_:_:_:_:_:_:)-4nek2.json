entifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V5"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","text":"OSCValueMaskable","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"content","content":[{"level":2,"type":"heading","anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Common usage:","type":"text"}]},{"type":"codeListing","syntax":null,"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"]},{"inlineContent":[{"text":"Basic examples:","type":"text"}],"type":"paragraph"},{"type":"codeListing","code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":null},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" is a special common-use interpolated type for convenience:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"These types conform to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","isActive":true},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"type":"reference","isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue"},{"text":" is accepted.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"OSCKit provides conformance for a number of such common Swift Standard Library types:","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"Int8"},{"text":", ","type":"text"},{"code":"Int16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt"},{"text":", ","type":"text"},{"code":"UInt8","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt16"},{"text":", ","type":"text"},{"code":"UInt32","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt64"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float16"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float80"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int64"},{"text":", ","type":"text"},{"code":"Float32","type":"codeVoice"},{"type":"text","text":" are already core "},{"code":"OSCValue","type":"codeVoice"},{"text":" types.)","type":"text"}]},{"syntax":null,"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue","type":"codeVoice"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}]},{"type":"codeListing","syntax":null,"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"type":"paragraph","inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"type":"codeVoice","code":"Optional"},{"text":" (ie: ","type":"text"},{"type":"codeVoice","code":"Int32?.self"},{"text":")","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"which will match that value type or return "},{"code":"nil","type":"codeVoice"},{"text":" if the value is missing in the base value","type":"text"},{"type":"text","text":" "},{"type":"text","text":"array."}]},{"inlineContent":[{"text":"Core OSC concrete types:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Float32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"String.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Data.self"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCTimeTag.self"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"items":[{"content":[{"inlineContent":[{"code":"Bool.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Character.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Int64.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Double.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}]}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"text":"Interpolating concrete types for convenience:","type":"text"}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" (converts from any OSC integer types)"}]}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"text":"Opaque types:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}],"type":"paragraph"}]}]},{"name":"Throws","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError"}]}],"type":"aside","style":"note"}]}],"abstract":[{"text":"Returns the OSC value sequence as a strongly typed tuple","type":"text"},{"type":"text","text":" "},{"type":"text","text":"if it matches the given mask of concrete value types."},{"text":" ","type":"text"},{"text":"If the value types do not match the mask, an error is thrown.","type":"text"}],"identifier":{"url":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift\/Array\/masked(_:_:_:_:_:_:)-4nek2","interfaceLanguage":"swift"},"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://osckit.OSCKit/documentation/OSCKit/OSCInterpolatedValue":{"kind":"symbol","title":"OSCInterpolatedValue","fragments":[{"text":"protocol","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"OSCInterpolatedValue"}],"role":"symbol","abstract":[{"text":"Protocol to which all interpolated OSC value types conform.","type":"text"},{"type":"text","text":" "},{"text":"This includes all convenience types that are not themselves core OSC value types.","type":"text"}],"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","url":"\/documentation\/osckit\/oscinterpolatedvalue","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OSCInterpolatedValue"}]},"doc://osckit.OSCKit/documentation/OSCKit/OSCValueMaskError":{"role":"symbol","navigatorTitle":[{"text":"OSCValueMaskError","kind":"identifier"}],"abstract":[{"text":"Error thrown by ","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValues","type":"reference","isActive":true},{"text":" ","ty