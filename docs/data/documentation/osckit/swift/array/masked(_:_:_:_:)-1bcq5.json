d":"text"},{"kind":"typeIdentifier","text":"V1"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V2"},{"kind":"text","text":", "},{"text":"V3","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"where","kind":"keyword"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V1"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V2"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V3"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"}]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Common usage:"}]},{"type":"codeListing","syntax":null,"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Basic examples:"}]},{"syntax":null,"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" is a special common-use interpolated type for convenience:","type":"text"}]},{"syntax":null,"type":"codeListing","code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"]},{"inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"text":" ","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage","isActive":true,"type":"reference"},{"text":".","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"These types conform to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","isActive":true},{"text":" protocol (which defines their core","type":"text"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue"},{"text":" is accepted.","type":"text"}]},{"inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"type":"text","text":" "},{"code":"Int8","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int16"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt"},{"text":", ","type":"text"},{"code":"UInt8","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt16"},{"text":", ","type":"text"},{"code":"UInt32","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt64","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float80"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int64"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float32"},{"text":" are already core ","type":"text"},{"type":"codeVoice","code":"OSCValue"},{"type":"text","text":" types.)"}],"type":"paragraph"},{"syntax":null,"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"text":" is a special type to match and box any OSC numeric value.","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"type":"codeVoice","code":"Optional"},{"text":" (ie: ","type":"text"},{"code":"Int32?.self","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"which will match that value type or return "},{"type":"codeVoice","code":"nil"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"array."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Core OSC concrete types:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int32.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Float32.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"String.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Data.self"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCTimeTag.self"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"items":[{"content":[{"inlineContent":[{"code":"Bool.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Character.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Double.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCArrayValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCNullValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}]}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int.self","type":"codeVoice"},{"text":" (converts from any OSC integer types)","type":"text"}]}]}]},{"inlineContent":[{"type":"text","text":"Opaque types:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}]},{"name":"Throws","content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError","isActive":true,"type":"reference"}]}],"type":"aside","style":"note"}]}],"metadata":{"extendedModule":"Swift","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"masked"},{"kind":"text","text":"<"},{"text":"V0","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"V1","kind":"genericParameter"},{"text":", ","kind":"text"},{"kind":"genericParameter","text":"V2"},{"text":", ","kind":"text"},{"kind":"genericParameter","text":"V3"},{"text":">(","kind":"text"},{"kind":"typeIdentifier","text":"V0"},{"text":".Type, ","kind":"text"},{"text":"V1","kind":"typeIdentifier"},{"text":".Type, ","kind":"text"},{"text":"V2","kind":"typeIdentifier"},{"kind":"text","text":".Type, "},{"text":"V3","kind":"type