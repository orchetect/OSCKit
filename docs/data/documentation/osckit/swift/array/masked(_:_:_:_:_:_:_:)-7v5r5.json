d":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V2"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V3"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V4"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V5"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V6"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","level":2,"type":"heading"},{"inlineContent":[{"type":"text","text":"A mask of up to 10 value types in sequence."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Common usage:"}],"type":"paragraph"},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"Basic examples:"}],"type":"paragraph"},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" is a special common-use interpolated type for convenience:","type":"text"}],"type":"paragraph"},{"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"text":" ","type":"text"},{"text":"be transparently encoded as their closest related OSC core type when encoding in an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"These types conform to the "},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","isActive":true,"type":"reference"},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue"},{"type":"text","text":" is accepted."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"type":"text","text":" "},{"code":"Int8","type":"codeVoice"},{"type":"text","text":", "},{"code":"Int16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt8"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt16"},{"type":"text","text":", "},{"code":"UInt32","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt64","type":"codeVoice"},{"type":"text","text":", "},{"code":"Float16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float80","type":"codeVoice"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"(Note that ","type":"text"},{"type":"codeVoice","code":"Int32"},{"type":"text","text":", "},{"code":"Int64","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float32"},{"type":"text","text":" are already core "},{"code":"OSCValue","type":"codeVoice"},{"type":"text","text":" types.)"}]},{"type":"codeListing","code":["let values: OSCValues = [Int8(123), Int16(123)]"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue","type":"codeVoice"},{"text":" is a special type to match and box any OSC numeric value.","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"code":"Int32?.self","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"which will match that value type or return "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if the value is missing in the base value"},{"type":"text","text":" "},{"type":"text","text":"array."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Core OSC concrete types:"}]},{"items":[{"content":[{"inlineContent":[{"code":"Int32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Float32.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"String.self"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Data.self"},{"type":"text","text":" (a.k.a. OSC blob)"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Bool.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Character.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Double.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCMIDIValue.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCStringAltValue.self","type":"codeVoice"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"text":"Interpolating concrete types for convenience:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int.self","type":"codeVoice"},{"text":" (converts from any OSC integer types)","type":"text"}]}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Opaque types:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}],"type":"unorderedList"},{"name":"Throws","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError"}]}],"type":"aside","style":"note"}]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/osckit.OSCKit\/documentation\/OSCKit","doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift","doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift\/Array"]]},"kind":"symbol","schemaVersion":{"patch":0,"minor":3,"major":0},"abstract":[{"text":"Returns the OSC value sequence as a strongly typed tuple","type":"text"},{"text":" ","type":"text"},{"text":"if it matches the given mask of concrete value types.","type":"text"},{"text":" ","type":"text"},{"text":"If the value types do not match the ma