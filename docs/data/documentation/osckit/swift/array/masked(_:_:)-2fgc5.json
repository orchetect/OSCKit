orms":["macOS"],"languages":["swift"],"tokens":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"masked","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V0","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"V1","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"kind":"internalParam","text":"v0"},{"text":": ","kind":"text"},{"text":"V0","kind":"typeIdentifier"},{"text":".Type, ","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"v1","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V1","kind":"typeIdentifier"},{"text":"?.Type) ","kind":"text"},{"kind":"keyword","text":"throws"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore17OSCValueMaskErrorO","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError","text":"OSCValueMaskError"},{"kind":"text","text":") -> ("},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":"?) "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V0"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V1"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"}]}],"kind":"declarations"},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"A mask of up to 10 value types in sequence."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Common usage:"}],"type":"paragraph"},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"Basic examples:"}],"type":"paragraph"},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage","type":"reference"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"These types conform to the "},{"isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","type":"reference"},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue","type":"reference"},{"type":"text","text":" is accepted."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"type":"text","text":" "},{"code":"Int8","type":"codeVoice"},{"type":"text","text":", "},{"code":"Int16","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt8","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt16","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt32","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt64","type":"codeVoice"},{"type":"text","text":", "},{"code":"Float16","type":"codeVoice"},{"type":"text","text":", "},{"code":"Float80","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"(Note that "},{"code":"Int32","type":"codeVoice"},{"type":"text","text":", "},{"code":"Int64","type":"codeVoice"},{"type":"text","text":", "},{"code":"Float32","type":"codeVoice"},{"type":"text","text":" are already core "},{"code":"OSCValue","type":"codeVoice"},{"type":"text","text":" types.)"}],"type":"paragraph"},{"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"code":"AnyOSCNumberValue","type":"codeVoice"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}],"type":"paragraph"},{"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"code":"Int32?.self","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"which will match that value type or return "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if the value is missing in the base value"},{"type":"text","text":" "},{"type":"text","text":"array."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Core OSC concrete types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"code":"Int32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Float32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"String.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Data.self"},{"text":" (a.k.a. OSC blob)","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCTimeTag.self"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Bool.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Character.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Double.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCArrayValue.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCNullValue.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" (converts from any OSC integer types)","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Opaque types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue.self"},{"text":" (boxes any OSC integer or float number)","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"name":"Throws","content":[{"inlineContent":[{"text":"","type":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError","isActive":true,"type":"reference"}],"type":"paragraph"}],"style":"note","type":"aside"}]}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/osckit.OSCKit\/documentation\/OSCKit","doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift","doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/Swift\/Array"]]},"schemaVersion":{"patch":0,"minor":3,"major":0},"references":{"doc://osckit.OSCKit/document