er":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","text":"OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V4"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","text":"OSCValueMaskable","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"V5"},{"kind":"text","text":" : "},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V6","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"}]}],"kind":"declarations"},{"content":[{"text":"Discussion","type":"heading","anchor":"discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"A mask of up to 10 value types in sequence."}]},{"type":"paragraph","inlineContent":[{"text":"Common usage:","type":"text"}]},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"type":"codeListing","syntax":null},{"type":"paragraph","inlineContent":[{"type":"text","text":"Basic examples:"}]},{"type":"codeListing","syntax":null,"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"]},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" is a special common-use interpolated type for convenience:","type":"text"}],"type":"paragraph"},{"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"type":"codeListing","syntax":null},{"inlineContent":[{"type":"text","text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage","type":"reference","isActive":true},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"These types conform to the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue"},{"type":"text","text":" protocol (which defines their core"},{"type":"text","text":" "},{"text":"encoding value type) and may be used any where ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue"},{"type":"text","text":" is accepted."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"OSCKit provides conformance for a number of such common Swift Standard Library types:","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"Int8"},{"text":", ","type":"text"},{"code":"Int16","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt8"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt16"},{"text":", ","type":"text"},{"code":"UInt32","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt64","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Float16","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float80"},{"type":"text","text":"."}]},{"inlineContent":[{"text":"(Note that ","type":"text"},{"type":"codeVoice","code":"Int32"},{"text":", ","type":"text"},{"code":"Int64","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float32"},{"text":" are already core ","type":"text"},{"code":"OSCValue","type":"codeVoice"},{"type":"text","text":" types.)"}],"type":"paragraph"},{"syntax":null,"type":"codeListing","code":["let values: OSCValues = [Int8(123), Int16(123)]"]},{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}],"type":"paragraph"},{"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"],"syntax":null,"type":"codeListing"},{"inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"type":"codeVoice","code":"Int32?.self"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"which will match that value type or return ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if the value is missing in the base value"},{"text":" ","type":"text"},{"type":"text","text":"array."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Core OSC concrete types:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int32.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Float32.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"String.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Data.self"},{"text":" (a.k.a. OSC blob)","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Bool.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Character.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Double.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"OSCMIDIValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCNullValue.self","type":"