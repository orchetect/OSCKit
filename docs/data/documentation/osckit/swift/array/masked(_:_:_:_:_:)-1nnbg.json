t":": "},{"kind":"typeIdentifier","text":"V3"},{"text":"?.Type, ","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"v4","kind":"internalParam"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V4"},{"kind":"text","text":"?.Type) "},{"text":"throws","kind":"keyword"},{"text":" -> (","kind":"text"},{"kind":"typeIdentifier","text":"V0"},{"text":", ","kind":"text"},{"text":"V1","kind":"typeIdentifier"},{"text":"?, ","kind":"text"},{"text":"V2","kind":"typeIdentifier"},{"text":"?, ","kind":"text"},{"kind":"typeIdentifier","text":"V3"},{"kind":"text","text":"?, "},{"kind":"typeIdentifier","text":"V4"},{"kind":"text","text":"?) "},{"text":"where","kind":"keyword"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V0"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"V1","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"text":"V2","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V3"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V4"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"}],"platforms":["macOS"],"languages":["swift"]}]},{"content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A mask of up to 10 value types in sequence."}]},{"type":"paragraph","inlineContent":[{"text":"Common usage:","type":"text"}]},{"type":"codeListing","syntax":null,"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"]},{"inlineContent":[{"text":"Basic examples:","type":"text"}],"type":"paragraph"},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":null,"type":"codeListing"},{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"type":"codeListing","code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"syntax":null},{"inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage","isActive":true},{"text":".","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"These types conform to the "},{"isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue","type":"reference"},{"text":" protocol (which defines their core","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"encoding value type) and may be used any where "},{"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue","isActive":true},{"type":"text","text":" is accepted."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"type":"text","text":" "},{"type":"codeVoice","code":"Int8"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int16"},{"type":"text","text":", "},{"code":"UInt","type":"codeVoice"},{"type":"text","text":", "},{"code":"UInt8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"UInt16","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt32"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt64"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float16"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float80"},{"text":".","type":"text"}]},{"inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"text":", ","type":"text"},{"code":"Int64","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float32"},{"type":"text","text":" are already core "},{"type":"codeVoice","code":"OSCValue"},{"text":" types.)","type":"text"}],"type":"paragraph"},{"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"type":"codeListing","syntax":null},{"inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"type":"paragraph","inlineContent":[{"text":"One or more trailing types can also be expressed as as an ","type":"text"},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"code":"Int32?.self","type":"codeVoice"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"which will match that value type or return "},{"code":"nil","type":"codeVoice"},{"text":" if the value is missing in the base value","type":"text"},{"text":" ","type":"text"},{"text":"array.","type":"text"}]},{"inlineContent":[{"type":"text","text":"Core OSC concrete types:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int32.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Float32.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"String.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Data.self"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"items":[{"content":[{"inlineContent":[{"code":"Bool.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"Character.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Double.self"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"code":"OSCStringAltValue.self","type":"codeVoice"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"text":"Interpolating concrete types for convenience:","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int.self"},{"text":" (converts from any OSC integer types)","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"text":"Opaque types:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}]},{"style":"note","type":"aside","content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"},{"type":"reference","identifier":"doc:\/\/os