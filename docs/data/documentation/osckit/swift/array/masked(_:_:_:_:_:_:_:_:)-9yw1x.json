nd":"text"},{"kind":"typeIdentifier","text":"OSCValueMaskable","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"}]}]},{"content":[{"type":"heading","anchor":"discussion","level":2,"text":"Discussion"},{"inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Common usage:"}],"type":"paragraph"},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"syntax":null,"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Basic examples:"}]},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":null,"type":"codeListing"},{"type":"paragraph","inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}]},{"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"],"type":"codeListing","syntax":null},{"type":"paragraph","inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage"},{"type":"text","text":"."}]},{"inlineContent":[{"text":"These types conform to the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue"},{"text":" protocol (which defines their core","type":"text"},{"type":"text","text":" "},{"type":"text","text":"encoding value type) and may be used any where "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue"},{"text":" is accepted.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"text":" ","type":"text"},{"code":"Int8","type":"codeVoice"},{"text":", ","type":"text"},{"code":"Int16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt8"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt16"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt32"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt64"},{"text":", ","type":"text"},{"code":"Float16","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float80"},{"text":".","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"(Note that ","type":"text"},{"code":"Int32","type":"codeVoice"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Int64"},{"type":"text","text":", "},{"code":"Float32","type":"codeVoice"},{"text":" are already core ","type":"text"},{"type":"codeVoice","code":"OSCValue"},{"text":" types.)","type":"text"}]},{"code":["let values: OSCValues = [Int8(123), Int16(123)]"],"syntax":null,"type":"codeListing"},{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue","type":"codeVoice"},{"type":"text","text":" is a special type to match and box any OSC numeric value."}]},{"type":"codeListing","syntax":null,"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"type":"codeVoice","code":"Int32?.self"},{"type":"text","text":")"},{"type":"text","text":" "},{"text":"which will match that value type or return ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if the value is missing in the base value"},{"type":"text","text":" "},{"text":"array.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Core OSC concrete types:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int32.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Float32.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"String.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Data.self","type":"codeVoice"},{"text":" (a.k.a. OSC blob)","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Bool.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Character.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Int64.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Double.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCArrayValue.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCImpulseValue.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCMIDIValue.self"}]}]},{"content":[{"inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}]}]}]},{"inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Int.self"},{"type":"text","text":" (converts from any OSC integer types)"}]}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Opaque types:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"text":" (boxes any OSC integer or float number)","type":"text"}]}]}],"type":"unorderedList"},{"type":"aside","style":"note","content":[{"inlineContent":[{"type":"text","text":""},{"type":"reference","isActiv