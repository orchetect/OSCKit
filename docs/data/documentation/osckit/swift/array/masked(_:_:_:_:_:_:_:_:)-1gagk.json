eciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","text":"OSCValueMaskable"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V1"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"V2","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V3","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","kind":"typeIdentifier","text":"OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"text":"V4","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP"},{"text":", ","kind":"text"},{"kind":"typeIdentifier","text":"V5"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"V6","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"OSCValueMaskable","kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable"},{"text":", ","kind":"text"},{"text":"V7","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:10OSCKitCore16OSCValueMaskableP","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskable","text":"OSCValueMaskable"}]}]},{"content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"A mask of up to 10 value types in sequence.","type":"text"}]},{"inlineContent":[{"text":"Common usage:","type":"text"}],"type":"paragraph"},{"code":["\/\/ as a tuple","let tuple = try values.masked(String.self, Int.self)","print(tuple.0, tuple.1)","","\/\/ unwrapped into local variables","let (string, int) = try values.masked(String.self, Int.self)","print(string, int)"],"type":"codeListing","syntax":null},{"inlineContent":[{"text":"Basic examples:","type":"text"}],"type":"paragraph"},{"code":["\/\/ present optional value","[\"Test\", 123, Int32(456)].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ 456 as Int32?","","\/\/ missing optional value","[\"Test\", 123].masked(String.self, Int32.self, Int32?.self)","tuple.0 \/\/ \"Test\" as String","tuple.1 \/\/ 123 as Int","tuple.2 \/\/ nil as Int32?","","\/\/ mismatching types","[123, 456].masked(String.self, Int32.self)","\/\/ throws OSCValueMaskError.mismatchedTypes","","\/\/ mismatched element count, regardless of types","[123, 456].masked(String.self, Int32.self, Int32.self)","\/\/ throws OSCValueMaskError.invalidCount"],"syntax":null,"type":"codeListing"},{"inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" is a special common-use interpolated type for convenience:"}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["\/\/ any integer may match against Int.self and is converted to Int","[Int32(123) Int64(456)].masked(Int.self, Int.self)","tuple.0 \/\/ 123 as Int","tuple.1 \/\/ 456 as Int","","\/\/ all other integer types must match themself exactly","[Int32(123) Int64(456)].masked(Int32.self, Int32.self)","\/\/ throws error; mask does not match","","\/\/ same with optionals; specific integer types must match if present","[123, 456, 789].masked(String.self, Int.self, Int32?.self)","\/\/ throws error; mask does not match"]},{"type":"paragraph","inlineContent":[{"text":"In addition to core OSC concrete types, various non-standard OSC types can be used and will","type":"text"},{"type":"text","text":" "},{"type":"text","text":"be transparently encoded as their closest related OSC core type when encoding in an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCMessage"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"These types conform to the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCInterpolatedValue"},{"text":" protocol (which defines their core","type":"text"},{"type":"text","text":" "},{"text":"encoding value type) and may be used any where ","type":"text"},{"isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValue","type":"reference"},{"text":" is accepted.","type":"text"}]},{"inlineContent":[{"type":"text","text":"OSCKit provides conformance for a number of such common Swift Standard Library types:"},{"type":"text","text":" "},{"code":"Int8","type":"codeVoice"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt8"},{"text":", ","type":"text"},{"type":"codeVoice","code":"UInt16"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt32"},{"type":"text","text":", "},{"type":"codeVoice","code":"UInt64"},{"type":"text","text":", "},{"type":"codeVoice","code":"Float16"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float80"},{"type":"text","text":"."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"(Note that "},{"type":"codeVoice","code":"Int32"},{"type":"text","text":", "},{"type":"codeVoice","code":"Int64"},{"text":", ","type":"text"},{"type":"codeVoice","code":"Float32"},{"type":"text","text":" are already core "},{"type":"codeVoice","code":"OSCValue"},{"text":" types.)","type":"text"}]},{"type":"codeListing","syntax":null,"code":["let values: OSCValues = [Int8(123), Int16(123)]"]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"AnyOSCNumberValue"},{"text":" is a special type to match and box any OSC numeric value.","type":"text"}]},{"type":"codeListing","syntax":null,"code":["[Int8(123)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Int8(123)))","","[Double(123.45)]","    .masked(AnyOSCNumberValue.self)","    \/\/ (AnyOSCNumberValue(Double(123.45)))","","\/\/ which can be accessed as either typed:","AnyOSCNumberValue(Int8(123)).base \/\/ .int(Int8)","","\/\/ or interpolated:","AnyOSCNumberValue(Int8(123)).intValue \/\/ 123 as Int","AnyOSCNumberValue(Int8(123)).doubleValue \/\/ 123.0 as Double"]},{"inlineContent":[{"type":"text","text":"One or more trailing types can also be expressed as as an "},{"code":"Optional","type":"codeVoice"},{"type":"text","text":" (ie: "},{"type":"codeVoice","code":"Int32?.self"},{"text":")","type":"text"},{"text":" ","type":"text"},{"text":"which will match that value type or return ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if the value is missing in the base value"},{"text":" ","type":"text"},{"text":"array.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Core OSC concrete types:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"Int32.self","type":"codeVoice"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"Float32.self"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"String.self"}]}]},{"content":[{"inlineContent":[{"code":"Data.self","type":"codeVoice"},{"type":"text","text":" (a.k.a. OSC blob)"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"OSCTimeTag.self","type":"codeVoice"}]}]}]},{"inlineContent":[{"type":"text","text":"Extended (non-standard) OSC concrete types:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"code":"Bool.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Character.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"code":"Int64.self","type":"codeVoice"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"Double.self"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"OSCArrayValue.self"}]}]},{"content":[{"inlineContent":[{"code":"OSCImpulseValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCMIDIValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"code":"OSCNullValue.self","type":"codeVoice"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"OSCStringAltValue.self"}],"type":"paragraph"}]}],"type":"unorderedList"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Interpolating concrete types for convenience:"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"code":"Int.self","type":"codeVoice"},{"type":"text","text":" (converts from any OSC integer types)"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Opaque types:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"AnyOSCNumberValue.self","type":"codeVoice"},{"type":"text","text":" (boxes any OSC integer or float number)"}]}]}]},{"type":"aside","style":"note","content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCValueMaskError"}]}],"name":"Throws"}],"kind":"content"}],"var