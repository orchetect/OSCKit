"aside","content":[{"inlineContent":[{"isActive":true,"identifier":"http:\/\/opensoundcontrol.org\/spec-1_0.html","type":"reference"},{"text":":","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"OSC server dispatches to all OSC Method subscribers that match the address pattern."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Address path components are known as Containers, and the final path component is the Method"},{"type":"text","text":" "},{"type":"text","text":"name."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Address patterns may or may not contain any wildcards. If no wildcards are contained, then the"},{"text":" ","type":"text"},{"type":"text","text":"address matches if it matches verbatim."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"All the matching OSC Methods are invoked with the same argument data, namely, the OSC"},{"text":" ","type":"text"},{"text":"Arguments in the OSC Message.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"An OSC Address Pattern matches an OSC Address if:","type":"text"}]},{"type":"orderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The OSC Address and the OSC Address Pattern contain the same number of parts; and"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Each part of the OSC Address Pattern matches the corresponding part of the OSC Address."}]}]}]}]}]},{"inlineContent":[{"text":"These are the matching rules for characters in the OSC Address Pattern:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"?","type":"codeVoice"},{"text":" in the OSC Address Pattern matches any single character","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"*"},{"text":" in the OSC Address Pattern matches any sequence of zero or more characters","type":"text"}]}]},{"content":[{"inlineContent":[{"code":"[chars]","type":"codeVoice"},{"type":"text","text":" - a string of characters in square brackets in the OSC Address Pattern matches any"},{"text":" ","type":"text"},{"type":"text","text":"character in the string. Inside square brackets, the minus sign ("},{"type":"codeVoice","code":"-"},{"type":"text","text":") and exclamation point"},{"type":"text","text":" "},{"type":"text","text":"("},{"code":"!","type":"codeVoice"},{"text":") have special meanings:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Two characters separated by a minus sign indicate the range of characters between the given"},{"text":" ","type":"text"},{"type":"text","text":"two in ASCII collating sequence. (A minus sign at the end of the string has no special"},{"text":" ","type":"text"},{"text":"meaning.)","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"An exclamation point at the beginning of a bracketed string negates the sense of the list,"},{"text":" ","type":"text"},{"type":"text","text":"meaning that the list matches any character not in the list. (An exclamation point anywhere"},{"type":"text","text":" "},{"type":"text","text":"besides the first character after the open bracket has no special meaning.)"}]}]}],"type":"unorderedList"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"{foo,bar}"},{"type":"text","text":" - A comma-separated list of strings enclosed in curly braces in the OSC Address"},{"type":"text","text":" "},{"type":"text","text":"Pattern matches any of the strings in the list."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Any other character in an OSC Address Pattern can match only the same character."}]}]}],"type":"unorderedList"}],"style":"note","name":"Note"},