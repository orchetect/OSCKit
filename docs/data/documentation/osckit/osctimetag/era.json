{"hierarchy":{"paths":[["doc:\/\/osckit.OSCKit\/documentation\/OSCKit","doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCTimeTag"]]},"sections":[],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCTimeTag\/era"},"kind":"symbol","abstract":[{"text":"NTP time format era number.","type":"text"}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"let","kind":"keyword"},{"kind":"text","text":" "},{"text":"era","kind":"identifier"},{"kind":"text","text":": "},{"text":"Int","kind":"typeIdentifier","preciseIdentifier":"s:Si"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"The OSC Time Tag encoding uses NTPv3 time encoding which specifies 32 bits for seconds and","type":"text"},{"text":" ","type":"text"},{"text":"32 bits for fractional sections. Given that the prime epoch (era 0) is the year 1990, 32 bit","type":"text"},{"text":" ","type":"text"},{"text":"seconds storage rolls over approximate every 136 years. Every time this rollover occurs, the","type":"text"},{"text":" ","type":"text"},{"text":"era increments by 1.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Hence, the period between 1 Jan 1900 and 7 Feb 2036 is considered era 0. Dates after 7 Feb","type":"text"},{"text":" ","type":"text"},{"text":"2036 for the following 136 years are considered era 1. And so on.","type":"text"}]},{"content":[{"inlineContent":[{"text":"See https:\/\/www.eecis.udel.edu\/~mills\/y2k.html for details.","type":"text"}],"type":"paragraph"}],"name":"Note","type":"aside","style":"note"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/osckit\/osctimetag\/era"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"externalID":"s:10OSCKitCore10OSCTimeTagV3eraSivp","symbolKind":"property","roleHeading":"Instance Property","role":"symbol","modules":[{"name":"OSCKit"}],"fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"era"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"title":"era"},"references":{"doc://osckit.OSCKit/documentation/OSCKit/OSCTimeTag":{"url":"\/documentation\/osckit\/osctimetag","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCTimeTag","kind":"symbol","abstract":[{"text":"OSC Time Tag.","type":"text"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"OSCTimeTag"}],"type":"topic","title":"OSCTimeTag","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"OSCTimeTag"}]},"doc://osckit.OSCKit/documentation/OSCKit/OSCTimeTag/era":{"url":"\/documentation\/osckit\/osctimetag\/era","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit\/OSCTimeTag\/era","kind":"symbol","abstract":[{"type":"text","text":"NTP time format era number."}],"role":"symbol","type":"topic","title":"era","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"era"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}]},"doc://osckit.OSCKit/documentation/OSCKit":{"type":"topic","identifier":"doc:\/\/osckit.OSCKit\/documentation\/OSCKit","url":"\/documentation\/osckit","title":"OSCKit","kind":"symbol","abstract":[{"type":"text","text":"Open Sound Control (OSC) library for macOS, iOS, tvOS and visionOS."}],"role":"collection"}}}